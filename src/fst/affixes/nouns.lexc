
! n1 = talo, basic paradigm
! n2 = liike, säe
! n3 = kanava
! n4 = veri

! nen = nainen paradigm
! 3nen = hevonen paradigm
! 3nks = keskus - keskuksen
! nas = tehas - tehtaan

LEXICON n1
 +N: x1 ;

LEXICON x1
 +Sg+Nom:^E2I       K ;
 +Sg+Gen:^WG%>n K ;
 +Sg+Par:%>^A K ;
 +Sg+Ill:^HMETA%>h^V^Vn K ;
 +Sg+Ine:^WG%>ss^A K ;
 +Sg+Ade:^WG%>l^A K ;
 +Pl+Nom:^WG%>t K ;
 +Pl+Gen:%>i2en K ;
 +Pl+Par:%>i2^A K ;
 +Pl+Ine:^WG%>i2ss^A K ;
 +Pl+Ill:^HMETA%>h^Vin K ;
 +Pl+Ade:^WG%>i2l^A K ;

LEXICON n2
 +N: x2 ;

LEXICON x2
 +Sg+Nom:^WG       K ;
 +Sg+Gen:h%>en     K ; ! liikheen, säkheen
 +Sg+Par:^WG%>tt^A K ; ! liikettä, säettä
 +Sg+Ill:h%>en     K ; ! liikheen, säkheen
 +Sg+Ine:%>ess^A   K ; ! liikheessä, säkheessä
 +Sg+Ade:%>el^A    K ; ! liikheelä, säkheelä
 +Sg+Abl:%>elt^A   K ; ! liikheeltä, säkheeltä
 +Pl+Nom:h%>et     K ;
 +Pl+Gen:h%>ejen   K ;
 +Pl+Par:h%>ej^A   K ;
 +Pl+Ill:%>en      K ; ! liikheen, säkheen
 +Pl+Ine:%>ess^A   K ; ! liikheessä, säkheessä
 +Pl+Ade:%>el^A    K ; ! liikheelä, säkheelä

LEXICON n3
 +N: x3 ;

LEXICON x3
 +Sg+Nom:^E2I       K ;
 +Sg+Gen:^WG%>n K ;
 +Sg+Par:%>^A K ;
 +Sg+Ill:^HMETA%>h^V^Vn K ;
 +Sg+Ine:^WG%>ss^A K ;
 +Sg+Ela:^WG%>st^A K ;
 +Sg+All:^WG%>lle K ;
 +Sg+Ade:^WG%>ll^A K ;
 +Sg+Abl:^WG%>lt^A K ;

 +Pl+Nom:^WG%>t K ;
