 +Pl+Ade:^WG%>i2ll^A K ;
 +Pl+Ade:^WG%>i2ll^A K ;
 +Pl+Abl:^WG%>i2lt^A K ;

  
LEXICON n4
 +N: x4 ;

LEXICON x4 ! veri
 +Sg+Nom:^E2I       K ; ! veri
 +Sg+Gen:^WG%>n     K ; ! veren
 +Sg+Par:%>t^A      K ; ! verta
 +Sg+Ine:^WG%>ss^A  K ; ! veressä
 +Sg+Ill:^HMETA%>h^V^Vn K ;
 +Sg+Ine:^WG%>ss^A K ;
 +Sg+Ade:^WG%>l^A K ;
