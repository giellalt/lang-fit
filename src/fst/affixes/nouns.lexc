 !+Pl+Tra:%>ei2 TRA ;
 !+Pl+Ess:%>ei2n^A PxK ;
 !+Pl+Abe:%>ei2tt^A PxK ;



LEXICON n3 
!! **LEXICON @LEXNAME@**  odd-syllabic: kanava
 +N: x3 ;
 +N+Nom:^E2I R ;
 +N+Gen:^WG%>n R ;

LEXICON n3_pl
!! **LEXICON @LEXNAME@**  haalarit
 +N: x3_pl ;

LEXICON x3
!! **LEXICON @LEXNAME@**
 x3_sg ;
 x3_pl ;

LEXICON x3_oblique
!! **LEXICON @LEXNAME@**
x3_oblique_sg ;
x3_pl ;

LEXICON x3_sg
!! **LEXICON @LEXNAME@**
 +Sg+Nom:^E2I       PxK ;
 x3_oblique_sg ;

LEXICON x3_oblique_sg
!! **LEXICON @LEXNAME@**
 +Sg+Gen:^WG%>n PxK ;
 +Sg+Par:%>^A PxK ;
 +Sg+Ill:^HMETA%>h^V^V n_PxK ;
